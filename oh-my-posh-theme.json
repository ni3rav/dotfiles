{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "style": "folder"
          },
          "template": "<#42E66C>\ueaf7 </><b>{{ .PWD }}</b>",
          "foreground": "#F1FA8C",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": ""
          },
          "template": "<#ffffff> ⚡ </><b>{{ .HEAD }}</b>",
          "foreground": "#43CCEA",
          "type": "git",
          "style": "diamond"
        },
        {
          "template": "<#ff0000> \u25c9</>",
          "foreground": "#ff0000",
          "type": "text",
          "style": "diamond"
        },
        {
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "environment",
            "fetch_virtual_env": true,
            "home_enabled": true
          },
          "style": "diamond",
          "template": "  {{ if .Venv }}({{ .Venv }}){{ end }} ",
          "type": "python"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "> ",
          "foreground": "#FFFFFF",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
