{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "closer": "p:os",
    "pink": "#F5C2E7",
    "lavender": "#B4BEFE",
    "blue": "#89B4FA"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        // {
        //   "foreground": "p:os",
        //   "style": "plain",
        //   "template": "{{.Icon}} ",
        //   "type": "os"
        // },
        // {
        //   "foreground": "p:blue",
        //   "style": "plain",
        //   "template": "{{ .UserName }}",
        //   "type": "session"
        // },
        {
          "foreground": "p:lavender",
          "style": "plain",
          "template": "{{ if .Venv }}\ue73c {{ .Venv }} {{ else }}\ue73c {{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "foreground": "p:lavender",
          "style": "plain",
          "template": "\ue256 {{ .Full }} ",
          "type": "java"
        },
        {
          "foreground": "p:pink",
          "options": {
            "folder_icon": "",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "style": "full"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:lavender",
          "options": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "foreground": "p:closer",
          "template": "\uf054",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
